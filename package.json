{
  "name": "@schemesonic/whats-new",
  "author": "SchemeSonic",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/whats-new.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/whats-new.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/whats-new.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "react-markdown": "^8.0.2",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1"
  },
  "peerDependencies": {
    "react": ">=16",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.1",
    "cssnano": "^5.0.14",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.3"
  }
}
